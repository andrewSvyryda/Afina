<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAuthenticateService" />
                <binding name="BasicHttpBinding_IUserService" />
                <binding name="BasicHttpBinding_ICourseService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:50303/AuthenticateService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAuthenticateService"
                contract="AuthenticateReference.IAuthenticateService" name="BasicHttpBinding_IAuthenticateService" />
            <endpoint address="http://localhost:50303/UserService.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IUserService" contract="UserService.IUserService"
                name="BasicHttpBinding_IUserService" />
            <endpoint address="http://localhost:50303/CourseService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICourseService"
                contract="CourseServiceReference.ICourseService" name="BasicHttpBinding_ICourseService" />
        </client>
    </system.serviceModel>
  <connectionStrings>
    <add name="AfinaDbContext" connectionString="data source=(LocalDb)\MSSQLLocalDB;initial catalog=DAL.AfinaDbContext;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>